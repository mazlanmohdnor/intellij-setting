<application>
  <component name="LanguageInjectionConfiguration">
    <injection language="Angular2HTML" injector-id="xml">
      <display-name>search-table/@id</display-name>
      <place><![CDATA[xmlAttribute().withLocalName(string().equalTo("id")).withParent(xmlTag().withLocalName(string().equalTo("search-table")).withNamespace(string().equalTo("http://www.w3.org/1999/html")))]]></place>
    </injection>
  </component>
</application>